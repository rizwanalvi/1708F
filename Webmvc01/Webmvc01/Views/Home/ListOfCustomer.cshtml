
@{
    ViewBag.Title = "ListOfCustomer";
}

<h2>ListOfCustomer</h2>

<table class="table">


    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Location</th>
            <th scope="col">Address</th>
            <th scope="col">Details</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in ViewBag.PageTitle)
            {

            <tr>


                <th scope="row">@item.Id</th>
                <td>@item.CUSTNAME</td>
                <td>@item.CUSTPH</td>
                <td>
                    <a href="#">Show</a>

                </td>
            </tr>
        }

    </tbody>
</table>